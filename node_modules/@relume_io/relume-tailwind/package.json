{
  "name": "@relume_io/relume-tailwind",
  "version": "1.3.0",
  "private": false,
  "sideEffects": [
    "*.css"
  ],
  "description": "Relume Tailwind Config. A preset Tailwind config using the Relume design system.",
  "keywords": [
    "relume",
    "tailwind",
    "tailwindcss",
    "design-system"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./dist/tailwind.config.mjs"
      },
      "require": {
        "default": "./dist/tailwind.config.js"
      }
    }
  },
  "author": {
    "name": "Relume",
    "email": "rob@relume.io",
    "url": "https://react-docs.relume.io/"
  },
  "homepage": "https://react-docs.relume.io/",
  "files": [
    "dist"
  ],
  "main": "./dist/tailwind.config.js",
  "module": "./dist/tailwind.config.mjs",
  "dependencies": {
    "@tailwindcss/typography": "0.5.15",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@swc/core": "1.3.100",
    "@types/node": "20.10.4",
    "rimraf": "5.0.5",
    "tailwindcss": "^3.4.10",
    "tsup": "8.0.1"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf .turbo node_modules dist"
  }
}