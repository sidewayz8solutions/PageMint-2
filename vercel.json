{
  "functions": {
    "api/create-checkout.js": {
      "maxDuration": 30
    },
    "api/verify-payment.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200
    }
  ]
}
